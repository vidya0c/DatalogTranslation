Q15) SELECT
	islocated.id,
	place1
FROM islocated
UNION
SELECT
	isconnectedto.id
	place1,
	dest
FROM isconnectedto
INNER JOIN dealswith ON isconnectedto.place2 = dealsWith.src AND dealsWith.dest='Japan'; 