Q11)SELECT
	ismarriedto.id
	person1,
	place
FROM ismarriedto
UNION
SELECT
	owns.id
	group,
	place2
FROM owns
INNER JOIN islocated ON owns.company = locatedin.place1 AND locatedin.place2='USA'; 

